{
  "name": "wizard-booth",
  "productName": "wizard-booth",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "parcel:watch": "parcel watch --target electron --public-url ./ --no-hmr src/*",
    "electron:start": "electron-forge start --app-path ./dist",
    "start": "concurrently -k \"npm:parcel:watch\" \"npm:electron:start\"",
    "package": "electron-forge package ./dist",
    "make": "electron-forge make -p linux ./dist",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Daniel Lawrence",
    "email": "lawrence.danielt@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "wizard_booth"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-flatpak",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.52",
    "@electron-forge/maker-deb": "^6.0.0-beta.52",
    "@electron-forge/maker-flatpak": "^6.0.0-beta.52",
    "@electron-forge/maker-rpm": "^6.0.0-beta.52",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.52",
    "@electron-forge/maker-zip": "^6.0.0-beta.52",
    "concurrently": "^5.2.0",
    "electron": "9.1.2",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-svelte": "^4.0.6",
    "sass": "^1.26.10",
    "svelte": "^3.24.0"
  }
}
